{
  "name": "EachChat",
  "version": "2.0.3",
  "author": "北京爱工作科技有限公司 <support@workly.ai>",
  "description": "亿洽",
  "license": "UNLICENSED",
  "private": true,
  "main": "./dist/electron/main.js",
  "homepage": "https://workly.ai/",
  "repository": "git@git.internal.yunify.com:YiQiLiao/YiQiLiao-Desktop.git",
  "scripts": {
    "build": "node --max_old_space_size=4096 .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "test": "mocha test/index.spec.js "
  },
  "build": {
    "productName": "EachChat",
    "appId": "com.worklyai.eachchat.desktop.testing",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "sign": false,
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build_mac/icons/icon.icns",
      "identity": "Beijing Yunify Technology Co., Ltd.",
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "build_mac/entitlements.mac.plist",
      "entitlementsInherit": "build_mac/entitlements.mac.plist"
    },
    "afterSign": "scripts/notarize.js",
    "win": {
      "icon": "./static/Img/logo.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "./static/Img/Main/logo@6x.png",
      "extraResources": [
        {
          "from": "./extralib/libcrypto.so",
          "to": "../"
        },
        {
          "from": "./extralib/libsqlcipher.so",
          "to": "../"
        },
        {
          "from": "./extralib/libssl.so",
          "to": "../"
        }
      ]
    },
    "npmRebuild": false
  },
  "dependencies": {
    "aws-sdk": "^2.698.0",
    "axios": "^0.18.0",
    "benz-amr-recorder": "^1.1.2",
    "bobolink": "^3.0.0",
    "browser-encrypt-attachment": "^0.3.0",
    "bufferutil": "^4.0.1",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "electron-log": "^4.2.4",
    "element-ui": "^2.4.9",
    "karma": "^5.0.9",
    "lodash": "^4.17.20",
    "matrix-js-sdk": "^8.4.1",
    "mocha": "^8.0.1",
    "mqtt": "^3.0.0",
    "olm": "../../olm-3.1.4.tgz",
    "pinyin": "^2.9.1",
    "quill": "^1.3.7",
    "require-dir": "^1.2.0",
    "sqlite3": "^4.2.0",
    "text-encoding-utf-8": "^1.0.2",
    "utf-8-validate": "^5.0.2",
    "v-viewer": "^1.5.1",
    "vue": "^2.5.16",
    "vue-cropper": "^0.5.5",
    "vue-electron": "^1.0.6",
    "vue-i18n": "^8.22.0",
    "vue-quill-editor": "^3.0.6",
    "vue-router": "^3.0.1",
    "vue-virtual-scroller": "^1.0.10",
    "vuex": "^3.0.1",
    "vuex-electron": "^1.0.0"
  },
  "devDependencies": {
    "ajv": "^6.5.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "cfonts": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.6",
    "crypto-js": "^4.0.0",
    "css-loader": "^0.28.11",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^10.1.3",
    "electron-builder": "^21.2.0",
    "electron-debug": "^1.5.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-notarize": "^1.0.0",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "js-base64": "^2.5.2",
    "jsencrypt": "^3.0.0-rc.1",
    "karma-chai": "^0.1.0",
    "karma-coverage": "^2.0.2",
    "karma-electron": "^6.3.0",
    "karma-mocha": "^2.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "0.4.0",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "node-sass": "^4.9.2",
    "sass-loader": "^7.0.3",
    "spectron": "12.0.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^15.2.4",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4",
    "webpack-hot-middleware": "^2.22.2",
    "webpack-merge": "^4.1.3"
  }
}
